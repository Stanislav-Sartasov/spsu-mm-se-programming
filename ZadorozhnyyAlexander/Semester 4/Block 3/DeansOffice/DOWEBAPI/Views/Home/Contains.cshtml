@model Exam
@{
	ViewData["Title"] = "Check results page";
}

<h1>
	Check results page
</h1>

<form method="post" asp-controller="Home" asp-action="Contains">
	<label asp-for="studentId"></label>
	<input asp-for="studentId" />
	<span asp-validation-for="studentId"></span>

	<label asp-for="courseId"></label>
	<input asp-for="courseId" />
	<span asp-validation-for="courseId"></span>

	<input type="submit" value="Check exam result" />
</form>
